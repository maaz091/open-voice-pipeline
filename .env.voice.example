# Voice Pipeline Service Configuration
# Copy this file to .env.voice and fill in your values

# STT (Gemini)
VOICE_STT_PROVIDER=whisper
VOICE_WHISPER_MODEL=small
VOICE_WHISPER_LANGUAGE=en

# LLM (Runpod)
VOICE_RUNPOD_BASE_URL=https://your-endpoint.runpod.net
VOICE_RUNPOD_MODEL=your-model-name
VOICE_RUNPOD_API_KEY=

# TTS (XTTS)
VOICE_TTS_SERVICE_URL=https://your-endpoint.runpod.net
VOICE_TTS_SPEED=1.0